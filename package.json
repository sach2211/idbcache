    "build": "rm -rf dist && babel lib --presets babel-preset-env --out-dir dist && uglifyjs dist/index.js --screw-ie8 -mc --output dist/index-min.js && exit 0",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
