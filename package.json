  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sach2211/idbcache/issues"
  },
  "homepage": "https://github.com/sach2211/idbcache#readme",
  "dependencies": {
    "idb-keyval": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "uglify-js": "^2.7.0"
  }
}
